<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PassWordConfirmActivity">


    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="0dp"
        android:layout_height="500dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/passWord_confirm_siteName_textView_print_id"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="@string/display_siteName" />

        <TextView
            android:id="@+id/passWord_confirm_siteName_textView_id"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="" />

        <TextView
            android:id="@+id/passWord_confirm_webSite_textView_print_id"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="@string/display_webSite" />

        <TextView
            android:id="@+id/passWord_confirm_webSite_textView_id"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="" />

        <TextView
            android:id="@+id/passWord_confirm_mailAddress_textView_print_id"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="@string/display_mailAddress" />

        <TextView
            android:id="@+id/passWord_confirm_mailAddress_textView_id"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="" />

        <TextView
            android:id="@+id/passWord_confirm_account_textView_print__id"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="@string/display_account" />

        <TextView
            android:id="@+id/passWord_confirm_account_textView_id"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="" />

        <TextView
            android:id="@+id/passWord_confirm_passWord_textView_print_id"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="@string/display_passWord" />

        <TextView
            android:id="@+id/passWord_confirm_passWord_textView_id"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="" />
    </LinearLayout>

    <Button
        android:id="@+id/password_confirm_save_button_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="146dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="177dp"
        android:text="@string/display_saveData"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

    <Button
        android:id="@+id/password_confirm_return_top_screen_button_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="149dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="174dp"
        android:text="@string/display_return_topScreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password_confirm_save_button_id" />



</androidx.constraintlayout.widget.ConstraintLayout>